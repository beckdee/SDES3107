//Use multiple translations to reposition a series of shapes.

size(200,200);
fill(0,40);
stroke(255,0,0);

ellipse(40, 40, 70, 70);
rect(10,80,40,40);
line(140,10,10,140);

translate(30, 30);
ellipse(40, 40, 70, 70);
rect(10,80,40,40);
line(140,10,10,140);

translate(30, 30);
ellipse(40, 40, 70, 70);
rect(10,80,40,40);
line(140,10,10,140);
